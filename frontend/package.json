{
  "name": "webgl2-shader-editor",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run gql-gen && pnpm run vite build && pnpm run html-php-rename",
    "html-php-rename": "powershell -Command \"Move-Item dist/index.html dist/index.php\"",
    "preview": "vite preview",
    "gql-gen": "graphql-codegen --config scripts\\gql-gen.ts",
    "gql-gen:watch": "graphql-codegen --config scripts\\gql-gen.ts --watch",
    "format": "prettier --config .prettierrc -w \"./src/**/*.{vue,ts}\""
  },
  "dependencies": {
    "@tweakpane/core": "^1.1.9",
    "@urql/vue": "^1.1.2",
    "fflate": "^0.8.1",
    "graphql": "^16.8.1",
    "idb-keyval": "^6.2.1",
    "less": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "monaco-editor": "0.36.1",
    "petite-vue-i18n": "9.9.0",
    "tweakpane": "^3.1.10",
    "vue": "^3.4.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.throttle": "^4.1.9",
    "@vitejs/plugin-vue": "^5.0.2",
    "dotenv": "^16.3.1",
    "html-minifier-terser": "^7.2.0",
    "lightningcss": "^1.25.1",
    "postcss-size": "^5.0.0",
    "prettier": "^3.1.1",
    "pug": "^3.0.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sitemap": "^7.1.1",
    "typescript": "^5.3.3",
    "unplugin-vue-macros": "^2.7.10",
    "vite": "^5.3.1",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-ssr": "^0.4.142",
    "vite-plugin-vue-css-modules": "^3.1.6",
    "vue-tsc": "^1.8.27"
  },
  "pnpm": {
    "patchedDependencies": {
      "monaco-editor@0.36.1": "patches/monaco-editor@0.36.1.patch"
    }
  }
}